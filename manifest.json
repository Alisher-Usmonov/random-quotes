{
    "name": "Random Quotes",
    "version": "1.0.0",
    "manifest_version": 3,
    "incognito": "split",
    "permissions": [
        "activeTab"
    ],
    "action": {
        "default_icon": "./images/icon.jpg"
    },
    "icons": {
        "16": "./images/icon.jpg",
        "32": "./images/icon.jpg",
        "64": "./images/icon.jpg",
        "128": "./images/icon.jpg"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    }
}